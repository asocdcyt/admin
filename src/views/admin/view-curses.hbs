  <main class="main-content position-relative max-height-vh-100 h-100 mt-1 border-radius-lg ">
    <!-- Navbar -->
    {{>navbar-main}}
    <!-- End Navbar -->
    <div class="container-fluid py-4">
      {{#if success_msg }}
            <div class="alert alert-success alert-dismissible fade show" role="alert">
                <span class="alert-icon text-white"><i class="ni ni-like-2"></i></span>
                <span class="alert-text text-white"><strong>¡Enhorabuena! -</strong> {{ success_msg }}</span>
                <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            {{/if}}
            {{#if error}}
              <div class="alert alert-danger alert-dismissible fade show" role="alert">
                  <span class="alert-icon text-white"><i class="ni ni-support-16"></i></span>
                  <span class="alert-text text-white"><strong>¡Oh no!</strong> {{error}}</span>
                  <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close">
                      <span aria-hidden="true">&times;</span>
                  </button>
              </div>
            {{/if}}
            {{#each errors }}
              <div class="alert alert-danger alert-dismissible fade show" role="alert">
                  <span class="alert-icon text-white"><i class="ni ni-support-16"></i></span>
                  <span class="alert-text text-white"><strong>¡Oh no!</strong> {{text}}</span>
                  <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close">
                      <span aria-hidden="true">&times;</span>
                  </button>
              </div>
            {{/each}}
            {{#if error_msg }}
              <div class="alert alert-danger alert-dismissible fade show" role="alert">
                  <span class="alert-icon text-white"><i class="ni ni-support-16"></i></span>
                  <span class="alert-text text-white"><strong>¡Oh no!</strong> {{error_msg}}</span>
                  <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close">
                      <span aria-hidden="true">&times;</span>
                  </button>
              </div>
            {{/if}}
      <div class="row">
        <div class="col-12 col-xl-12">
          <div class="card h-100">
            <div class="card-body p-3">
              <div class="row">
                <div class="col-6 text-start">
                <h6 class="heading-small text-muted mb-4">ID de Solicitud: {{dash._id}} </br>
                Estado: {{dash.status}}
                </h6>
              </div>
              <div class="col-6 text-end">
                <button type="button" class="btn btn-icon btn-3 btn-warning text-dark" data-bs-toggle="modal" data-bs-target="#ModalLongProfile"><span class="btn-inner--icon"><i class="fas fa-user-edit"></i></span>
                      <span class="btn-inner--text"> Cambiar Estado</span></button>
                <button type="button" class="btn btn-icon btn-3 btn-danger text-dark" data-bs-toggle="modal" data-bs-target="#ModalLongProfile1"><span class="btn-inner--icon"><i class="fas fa-user-x"></i></span>
                      <span class="btn-inner--text"> Eliminar</span></button>
              </div>
              </div>
              
              <form>
                <div class="pl-lg-4">
                  <div class="row">
                    <div class="col-lg-6">
                      <div class="form-group">
                        <label class="form-control-label">Nombre completo</label>
                        <input type="text" disabled class="form-control" maxlength="100" value="{{dash.fullname}}">
                      </div>
                    </div>
                    <div class="col-lg-6">
                      <div class="form-group">
                        <label class="form-control-label">Correo electrónico</label>
                        <input type="email"  disabled class="form-control" value="{{dash.email}}">
                      </div>
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-lg-12">
                      <div class="form-group">
                        <label class="form-control-label">Número de whatsApp</label>
                        <input type="text" disabled class="form-control" value="{{dash.ws}}">
                      </div>
                    </div>
                    
                  </div>
                  <div class="row">
                    <div class="col-lg-6">
                      <div class="form-group">
                        <label class="form-control-label">Número de teléfono</label>
                        <input type="number" disabled class="form-control" value="{{dash.phone}}">
                      </div>
                    </div>
                    <div class="col-lg-6">
                      <div class="form-group">
                        <label class="form-control-label">N° C.I. / R.I.F.</label>
                        <input type="number" disabled class="form-control" value="{{dash.idcard}}">
                      </div>
                    </div>
                    <div class="col-lg-6">
                      <div class="form-group">
                        <label class="form-control-label">Curso a Inscribir</label>
                        <input type="text" disabled class="form-control" value="{{dash.curse}}">
                      </div>
                    </div>
                    <div class="col-lg-6">
                      <div class="form-group">
                        <label class="form-control-label">Fecha y hora de transferencia (VEN)</label>
                        <input type="text" disabled class="form-control" value="{{dash.fulltime}}">
                      </div>
                    </div>
                    <div class="col-lg-6">
                      <div class="form-group">
                        <label class="form-control-label">Banco donde realizó la transferencia</label>
                        <input type="text" disabled class="form-control" value="{{dash.bankref}}">
                      </div>
                    </div>
                    <div class="col-lg-6">
                      <div class="form-group">
                        <label class="form-control-label">Monto de la transferencia</label>
                        <input type="text" disabled class="form-control" value="{{dash.amountref}}">
                      </div>
                    </div>
                    <div class="col-lg-6">
                      <div class="form-group">
                        <label class="form-control-label">Número de Referencia</label>
                        <input type="text" disabled class="form-control" value="{{dash.reference}}">
                      </div>
                    </div>
                    <div class="col-lg-6">
                      <div class="form-group">
                        <label class="form-control-label">Estado de solicitud</label>

                        <input type="text" disabled class="form-control" value="{{dash.status}}">
                      </div>
                    </div>
                  </div>
                </div>
              </form>
            </div>
          </div>
        </div>
      </div>
    </div>
    
    <div class="modal fade" id="ModalLongProfile" tabindex="-1" role="dialog" aria-labelledby="exampleModalLongTitle" aria-hidden="true">
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title" id="exampleModalLongTitle">Editar Información de Contácto</h5>
            <button type="button"  class="btn-close text-dark" data-bs-dismiss="modal" aria-label="Close">
              <span aria-hidden="true">&times;</span>
            </button>
          </div>
          <div class="modal-body">
              <form action="/data/edit-curse/{{dash._id}}?_method=PUT" method="POST">
                <input type="hidden" name="_method" value="PUT">
                  <div class="col-lg-12 text-center">
                      <div class="form-group">
                        <label class="form-control-label">Seleccione el estado:</label>
                        <select class="form-select form-control" type="txt" name="status" value="">
                            <option value="PENDIENTE" style="color: black">PENDIENTE</option>
                            <option value="RECHAZADO" style="color: black">RECHAZADO</option>
                            <option value="CONFIRMADO" style="color: black">CONFIRMADO</option>
                        </select>
                      </div>
                    </div>
                  <div class="col-md-12 text-center">
                    <button type="submit" class="btn btn-primary">Cambiar estado</button>
                  </div>
              </form>
          </div>
        </div>
      </div>
    </div>

    <div class="modal fade" id="ModalLongProfile1" tabindex="-1" role="dialog" aria-labelledby="exampleModalLongTitle" aria-hidden="true">
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title" id="exampleModalLongTitle">Eliminar solititud</h5>
            <button type="button"  class="btn-close text-dark" data-bs-dismiss="modal" aria-label="Close">
              <span aria-hidden="true">&times;</span>
            </button>
          </div>
          <div class="modal-body">
              <form action="/data/delete-curse/{{dash._id}}?_method=DELETE" method="POST">
                <input type="hidden" name="_method" value="DELETE">
                <div class="row m-4">
                  <div class="col-lg-12 text-center">
                      <h3>¿Está seguro que desea eliminar la solicitud?</br>
                      Correo: {{dash.email}}
                      </h3>
                    </div>
                </div>
                <div class="row m-4">
                  <div class="col-md-12 text-center">
                    <button type="submit" class="btn btn-danger">Si, Borrar permanentemente</button>
                  </div>
                </div>
                  
              </form>
          </div>
        </div>
      </div>
    </div>

      {{>footer-b}}
    </div>
  </main>